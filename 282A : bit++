#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    int X = 0;
    cin >> n;
    string a;
    for(int i = 0; i<n; i++){
        cin >> a;
        if(a == "X++" || a == "++X" ){
            X++;
        }
        else if(a == "X--" || a == "--X" ){
            X--;
        }
    }
    cout << X;
    return 0;
}
