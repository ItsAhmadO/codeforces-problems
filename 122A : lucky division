#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    int n;
    cin >> n;
    int a[13] = {4,7,47,74,77,44,444,447,474,477,777,774,744};
    bool t = false;
    for(int i = 0; i<13;i++){
        if(n % a[i] == 0){
            t = true;
            break;
        }
    }
    if(t){
        cout << "YES";
    }
    else{
        cout << "NO";
    }
    return 0;
}
