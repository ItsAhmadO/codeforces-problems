#include <bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin >> n;
    int counter = 0;
    while(n--){
        int a,b,c;
        cin >> a >> b >> c;
        if(a == 1 && b == 1){
            counter += 1;
        }
        else if(b == 1 && c == 1){
            counter += 1;
        }
        else if(a == 1 && c == 1){
            counter += 1;
        }
        else{
            continue;
        }
    }
    cout << counter;
    return 0;
}
