#include <bits/stdc++.h>
using namespace std;
#define ll long long

int main() {
    int arr[5][5], d;
    for(int i = 0; i<5; i++){
        for(int j = 0; j<5; j++){
            cin >> arr[i][j];
            if(arr[i][j] == 1){
                d = abs(i - 2) + abs(j-2);  
                break;   
            }
        }
    }
    cout << d;
    return 0;
}
