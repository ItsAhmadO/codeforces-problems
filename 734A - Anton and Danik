#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
    int n;
    int ac = 0;
    int bc = 0;
    cin >> n;
    string s;
    cin >> s;
    for(int i = 0; i < n; i++){
        if(s[i] == 'A'){
            ac ++;
        }
        else{
            bc++;
        }
    }

    if(ac > bc){
        cout << "Anton";
    }
    else if(ac < bc){
        cout << "Danik";
    }
    else{
        cout << "Friendship";
    }
    return 0;
}
