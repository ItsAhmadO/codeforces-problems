#include <bits/stdc++.h>
using namespace std;
#define ll long long
int main(){
    string s;
    cin >> s;
    string a = "hello";
    int x=0;
    int count=0;
    for(int i = 0; i < s.length(); i++){
        if(s[i] == a[x]){
            x++;
            count++;
            if(count == 5){
                break;
            }
        }
    }
    if(count == 5){
        cout << "YES";
    }
    else{
        cout << "NO";
    }

    return 0;
}
